            </dlentry>
            <dlentry id="fixtures" class="- topic/dlentry " xtrf="file:/Users/jmckay/Documents/Content/ncs_content/9780130210100/topics/english/M01_topic_4_4_1.xml" xtrc="dlentry:16;66:36">
